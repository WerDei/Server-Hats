  "authors": [
    "WerDei"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.werdei.serverhats.ServerHats"
    ]
  },
  "mixins": [
    "server-hats.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": "*",
    "minecraft": [
      ">=1.17"
    ]
  }
}
